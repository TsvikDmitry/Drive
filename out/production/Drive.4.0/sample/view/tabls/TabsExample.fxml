<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="1000.0" minWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.tabls.AddTabsController">


   <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
         <TabPane fx:id="tabPane" VBox.vgrow="ALWAYS" />
         <HBox alignment="center">
            <Button onAction="#addTab" text="Add tab" />
            <Button onAction="#listTabs" prefHeight="0.0" prefWidth="77.0" text="List tabs" />
         </HBox>
      </children>
   </VBox>
</AnchorPane>
